<div class="article">

  <span>
    <%= @article.raw_article.title %>
  </span>
  <br/>
  <%= @article.authors %>
  <br/>
  <span>
    Abstract
  </span>
  <p>
    <%= @article.raw_article.abstract %>
  </p>
  <div class="tag_container">
    <span>Tags</span>
    <br/>
    <% @article.tags.each do |tag| %>
      <span>[<%= tag.name %>]</span>
    <% end %>
  </div>
</div>
